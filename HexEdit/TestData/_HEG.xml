<?xml version="1.0"?>
<!DOCTYPE binary_file_format SYSTEM "BinaryFileFormat.dtd">
<binary_file_format name="HEG_File" type_name="" comment="" display="">
	<define_struct type_name="square" comment="" display="({this.dx} X {this.dy})  at ({this.x}, {this.y})"><data type="int" name="flags" format="unsigned" len="1" display="hex" domain="this == 0"/><data type="int" name="colid" format="unsigned" len="2" display=""/><data type="real" name="x" format="ieee" len="4" display=""/><data type="real" name="y" format="ieee" len="4" display=""/><data type="real" name="dx" format="ieee" len="4" display="" domain="this &gt; 0.0"/><data type="real" name="dy" format="ieee" len="4" domain="this &gt; 0.0" display=""/></define_struct><define_struct type_name="circle" comment="" display="radius=this.radius at ({this.x}, {this.y})"><data type="int" name="flags" format="unsigned" len="1" display="hex" domain="this == 0"/><data type="int" name="colid" format="unsigned" len="2" display=""/><data type="real" name="x" format="ieee" len="4" display=""/><data type="real" name="y" format="ieee" len="4" display=""/><data type="real" name="radius" format="ieee" len="4" display="" domain="this &gt; 0.0"/></define_struct><define_struct type_name="group" comment="" display="{this.count} sub-objects"><data type="int" name="flags" format="unsigned" len="1" display="hex" domain="this == 0"/><data type="int" name="count" format="unsigned" len="2" display="" comment="Number of grouped objects."/><data type="real" name="x" format="ieee" len="4" display=""/><data type="real" name="y" format="ieee" len="4" display=""/><for name="object" count="count" stop_test="" type_name="" comment=""><struct name="" type_name="" comment="" display=""><data type="int" name="next_type" format="signed" len="1" display=""/><if test="next_type == 1" comment=""><use_struct name="sub_square" display="" type_name="square" comment=""/></if><if test="next_type == 2" comment=""><use_struct name="sub_circle" display="" type_name="circle" comment=""/></if><if test="next_type == 10" comment=""><use_struct name="sub_group" display="" type_name="group" comment=""/></if></struct></for></define_struct><struct name="header" type_name="" comment="" display=""><data type="int" name="magic" format="unsigned" len="4" domain="this == 0x16071958" display="hex" comment="HEG file magic number - should always be 16071958 hex."/><data type="date" name="created" format="c" display="" comment="When the image was created."/><data type="date" name="modified" format="c" display="" comment="When the image was last changed." domain="this &gt; created"/><data type="int" name="colour_table_address" format="unsigned" len="4" display="hex" comment="Address in the file of the colours table."/></struct><for name="object" count="" stop_test="this.next_type == 0" type_name="" comment=""><struct name="" type_name="" comment="" display="{this.next_type == 1 ? &quot;SQUARE&quot; : (this.next_type == 2 ? &quot;CIRCLE&quot; : (this.next_type == 10 ? &quot;GROUP&quot; : &quot;END&quot;))}"><data type="int" name="next_type" format="unsigned" len="1" display=""/><if test="next_type == 1" comment=""><use_struct name="square" display="" type_name="square" comment=""/></if><if test="next_type == 2" comment=""><use_struct name="circle" display="" type_name="circle" comment=""/></if><if test="next_type == 10" comment=""><use_struct name="group" display="" type_name="group" comment=""/></if></struct></for><data type="none" name="dummy" len="1" comment="Extra byte for word alignment."/><data type="int" name="num_cols" format="unsigned" len="2" domain="this &gt; 0" display=""/><for name="col" count="num_cols" stop_test="" type_name="" comment=""><struct name="" type_name="" comment="" display=""><data type="string" name="name" format="ascii" len="13" display=""/><data type="int" name="red" format="unsigned" len="1" display=""/><data type="int" name="green" format="unsigned" len="1" display=""/><data type="int" name="blue" format="unsigned" len="1" display=""/></struct></for></binary_file_format>
